{
    "manifest_version": 2,
    "name" : "Dark Web",
    "version" :"1.0",
    "description" : "Dark Web exention to make all the websites dark",
    "short_name" : "Dark Web"
    "browser_action": {
        "default_icon": "icon.png"
        
      },
      "icons": { "32": "icon.png" },
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "css":[
                "style.css"
            ],
            "js":[
                "content.js"
            ]
        }
    ]
}